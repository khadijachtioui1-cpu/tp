#include <stdio.h>

int main() {
    int a, b, c;

    printf("Entrez trois nombres : ");
    scanf("%d %d %d", &a, &b, &c);

    // On applique l'algorithme d'Euclide sur les 3 nombres
    while (a != b || b != c) {
        if (a > b) a = a - b;
        else if (b > a) b = b - a;

        if (b > c) b = b - c;
        else if (c > b) c = c - b;
    }

    // Ã€ la fin a = b = c = PGCD
    printf("Le PGCD des trois nombres est : %d\n", a);

    return 0;
}
